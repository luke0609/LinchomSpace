<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mine.Set_activity"
    >
    <RelativeLayout
        android:id="@+id/rl_she"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="#f5f5f5"
        >
        <ImageView
            android:id="@+id/iv_back"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:src="@drawable/article_back"
            />
        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设 置"
            android:textColor="#212121"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:textSize="17sp"
            />
    </RelativeLayout>
    <View
        android:id="@+id/v_h1"
        android:layout_width="match_parent"
        android:layout_height="0.2dp"
        android:background="#808080"
        android:layout_below="@id/rl_she"/>

    <RelativeLayout
        android:id="@+id/rl_person"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_below="@+id/rl_she"
        >
        <TextView
            android:id="@+id/tv_person"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="个人资料"
            android:textColor="#212121"
            android:textSize="18sp"
            android:layout_marginLeft="20dp"
            android:layout_centerVertical="true"
            />
        <ImageView
            android:id="@+id/iv_go"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:src="@drawable/set_back2"
            android:layout_centerVertical="true"
            android:layout_marginRight="8dp"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        android:id="@+id/v_v1"
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#ccc"
        android:layout_marginLeft="18dp"
        android:layout_below="@id/rl_person"/>

    <RelativeLayout
        android:id="@+id/rl_safy"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_below="@+id/v_v1"
        >
        <TextView
            android:id="@+id/tv_safy"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="账号与安全"
            android:textColor="#212121"
            android:textSize="18sp"
            android:layout_marginLeft="20dp"
            android:layout_centerVertical="true"
            />

        <ImageView
            android:id="@+id/iv_go2"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginRight="8dp"
            android:src="@drawable/set_back2"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        android:id="@+id/v_v2"
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#808080"
        android:layout_marginLeft="18dp"
        android:layout_below="@id/rl_safy"/>

    <RelativeLayout
        android:id="@+id/rl_note"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_below="@+id/v_v2"
        >
        <TextView
            android:id="@+id/tv_note"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="隐私"
            android:textSize="18sp"
            android:textColor="#212121"
            android:layout_marginLeft="20dp"
            android:layout_centerVertical="true"
            />

        <ImageView
            android:id="@+id/iv_go3"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginRight="8dp"
            android:src="@drawable/set_back2"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        android:id="@+id/v_v3"
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#808080"
        android:layout_marginLeft="18dp"
        android:layout_below="@id/rl_note"/>
    <RelativeLayout
        android:id="@+id/rl_pri"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_below="@+id/v_v3"
        >
        <TextView
            android:id="@+id/tv_pri"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="通用"
            android:textColor="#212121"
            android:textSize="18sp"
            android:layout_marginLeft="20dp"
            android:layout_centerVertical="true"
            />

        <ImageView
            android:id="@+id/iv_go4"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginRight="8dp"
            android:src="@drawable/set_back2"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        android:id="@+id/v_v4"
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#808080"
        android:layout_marginLeft="18dp"
        android:layout_below="@id/rl_pri"/>
